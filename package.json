{
  "name": "react-notes-app",
  "version": "0.1.0",
  "homepage": "http://notes.jswebdev.net",
  "repository": {
    "type": "git",
    "url": "https://github.com/severjason/react-notes-app.git"
  },
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.0.2",
    "@material-ui/icons": "^3.0.1",
    "@types/material-ui": "^0.21.5",
    "node-sass-chokidar": "^1.2.2",
    "npm-run-all": "^4.1.2",
    "react": "^16.4.2",
    "react-dom": "^16.3.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts-ts": "2.17.0",
    "redux": "^4.0.0",
    "redux-persist": "^5.9.1",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "semantic-ui-css": "^2.3.3",
    "semantic-ui-react": "^0.82.3",
    "styled-components": "^3.4.5",
    "uuid": "^3.2.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts-ts start",
    "start_old": "npm-run-all -p watch-css start-js",
    "start": "react-scripts-ts start",
    "build": "npm run build-css && react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/styled-components": "^3.0.0",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.9.4",
    "@types/react": "^16.4.13",
    "@types/react-dom": "^16.0.7",
    "@types/react-helmet": "^5.0.5",
    "@types/react-redux": "^6.0.6",
    "@types/react-router-dom": "^4.2.6",
    "@types/redux-logger": "^3.0.5",
    "@types/uuid": "^3.4.3",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "typescript": "^3.0.3"
  }
}
